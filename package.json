        "@types/loglevel": "^1.5.3",
        "@types/mocha": "^5.2.3",
        "@types/node": "^12.0.2",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "grunt": "^1.0.3",
        "grunt-contrib-clean": "^1.1.0",
